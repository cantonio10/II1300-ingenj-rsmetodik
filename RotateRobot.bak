#define WHEEL_RADIUS 2.75
#define WHEEL_DISTANCE 18.3

sub RotateRobot(float deg)
{
     float wheelCir = 2 * WHEEL_RADIUS * 3.141592;
     float circleCir = WHEEL_DISTANCE * 3.141592;

     float rotateDegrees = (wheelCir / circleCir) * (deg / ((wheelCir / circleCir) * 360));

     RotateMotorEx(OUT_AB, 100, deg, 100, true, true);
}
